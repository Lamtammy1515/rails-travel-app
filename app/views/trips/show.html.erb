<div class="page-container prev-next">
  <a href="#" class="js-previous-trip" data-user-id="<%= @user.id %>" data-trip-id="<%= @trip.id %>">&laquo; Previous</a>
  <a href="#" class="js-next-trip" data-user-id="<%= @user.id %>" data-trip-id="<%= @trip.id %>">Next &raquo;</a>

  <h3><span class="tripCity"><%= @trip.city.city_plus_country %></span></h3>
  <p><span class="bold-text">Rating: </span><span class="tripRating"><%= @trip.rating %></span></p>
  <p><span class="bold-text">Must See Attraction: </span><span class="tripMustSee"><%= @trip.fave_attraction %></span></p>
  <p><span class="bold-text">Review: </span><span class="tripReview"><%= @trip.comment %></span></p><br>
</div>
